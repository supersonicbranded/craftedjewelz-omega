{
  "name": "craftedjewelz",
  "version": "2.0.5",
  "description": "CraftedJewelz â€“ Desktop App for 3D Jewelry Design",
  "author": "Supersonic Branded <support@supersonicbranded.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/supersonicbranded/craftedjewelz-omega.git"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electronmon .",
    "build": "electron-builder"
  },
  "dependencies": {
    "electronmon": "^2.0.2"
  },
  "devDependencies": {
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.supersonicbranded.craftedjewelz",
    "productName": "CraftedJewelz",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "apps/web/build/icons/appicon.icns",
      "target": ["dmg", "zip"]
    },
    "win": {
      "icon": "apps/web/build/icons/appicon.ico",
      "target": ["nsis", "zip"]
    },
    "linux": {
      "category": "Utility",
      "icon": "apps/web/build/icons",
      "target": ["AppImage", "deb"]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "supersonicbranded",
        "repo": "craftedjewelz-omega"
      }
    ]
  }
}


