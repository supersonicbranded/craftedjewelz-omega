{
  "name": "craftedjewelz",
  "version": "2.0.6",
  "description": "CraftedJewelz Desktop App",
  "author": "supersonicbranded",
  "main": "main.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:start-renderer\" \"npm:start-electron\"",
    "start-renderer": "cd renderer && npm start",
    "start-electron": "electron .",
    "build": "npm run build-renderer && electron-builder",
    "build-renderer": "cd renderer && npm run build"
  },
  "build": {
    "appId": "com.craftedjewelz.desktop",
    "productName": "CraftedJewelz",
    "files": [
      "dist/",
      "main.js",
      "preload.js",
      "index.html",
      "build/"
    ],
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "supersonicbranded",
        "repo": "craftedjewelz-omega"
      }
    ]
  },
  "dependencies": {
    "electron-is-dev": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "concurrently": "^8.2.2"
  }
}

    "electron-builder": "^24.13.3"
  },
  "dependencies": {}
}



