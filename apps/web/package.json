{
  "name": "craftedjewelz",
  "version": "3.0.2",
  "description": "CraftedJewelz Desktop App",
  "author": {
    "name": "supersonicbranded",
    "email": "support@craftedjewelz.com"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.supersonicbranded.craftedjewelz",
    "productName": "CraftedJewelz",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "package.json"
    ],
    "extraResources": [
      "assets/**"
    ],
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": ["nsis", "zip"],
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "maintainer": "supersonicbranded <support@craftedjewelz.com>",
      "icon": "build/icons",
      "category": "Utility"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "supersonicbranded",
        "repo": "craftedjewelz-omega"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supersonicbranded/craftedjewelz-omega.git"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {}
}
